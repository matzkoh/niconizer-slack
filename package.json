{
  "name": "niconizer-slack",
  "version": "0.0.0-development",
  "description": "CLI that watch slack messages and send to niconizer.",
  "keywords": [
    "comment",
    "niconizer",
    "slack"
  ],
  "repository": "github:matzkoh/niconizer-slack",
  "license": "MIT",
  "main": "dist/lib",
  "types": "dist/lib",
  "bin": {
    "niconizer-slack": "dist/bin/index.js"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "fix": "run-p fix:*",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "postinstall": "husky install",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier -c .",
    "lint:spell": "cspell --gitignore --no-progress .",
    "lint:type": "tsc --noEmit",
    "test": "run-s lint build"
  },
  "dependencies": {
    "@slack/rtm-api": "6.2.1",
    "@slack/web-api": "7.0.4",
    "commander": "12.1.0",
    "emoji-datasource": "15.1.2",
    "slack-message-parser": "3.0.2",
    "ws": "8.17.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "7.12.0",
    "@typescript-eslint/parser": "7.12.0",
    "cspell": "8.8.4",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "husky": "9.0.11",
    "lint-staged": "15.2.5",
    "npm-run-all2": "6.2.0",
    "prettier": "3.3.1",
    "prettier-plugin-packagejson": "2.5.0",
    "rimraf": "5.0.7",
    "typescript": "5.4.5"
  },
  "engines": {
    "node": ">= 18",
    "npm": ">= 9"
  }
}
