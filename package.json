{
  "name": "niconizer-slack",
  "version": "0.0.0-development",
  "description": "CLI that watch slack messages and send to niconizer.",
  "keywords": [
    "comment",
    "niconizer",
    "slack"
  ],
  "repository": "github:matzkoh/niconizer-slack",
  "license": "MIT",
  "main": "dist/lib",
  "types": "dist/lib",
  "bin": {
    "niconizer-slack": "dist/bin/index.js"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "fix": "run-p fix:*",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint --ext js,ts .",
    "lint:prettier": "prettier -l \"**/*.{json,md,yml}\"",
    "lint:type": "tsc --noEmit",
    "test": "run-s lint build"
  },
  "dependencies": {
    "@slack/rtm-api": "5.0.5",
    "@slack/web-api": "5.14.0",
    "commander": "6.2.1",
    "emoji-datasource": "6.0.0",
    "slack-message-parser": "1.0.7",
    "ws": "7.4.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "4.11.0",
    "@typescript-eslint/parser": "4.11.0",
    "eslint": "7.16.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.3.0",
    "eslint-plugin-simple-import-sort": "5.0.3",
    "husky": "4.3.6",
    "lint-staged": "10.5.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "prettier-plugin-packagejson": "2.2.8",
    "rimraf": "3.0.2",
    "typescript": "4.1.3"
  },
  "engines": {
    "node": ">= 12",
    "npm": ">= 6.14"
  }
}
