{
  "name": "niconizer-slack",
  "version": "0.0.0-development",
  "description": "CLI that watch slack messages and send to niconizer.",
  "keywords": [
    "comment",
    "niconizer",
    "slack"
  ],
  "repository": "github:matzkoh/niconizer-slack",
  "license": "MIT",
  "main": "dist/lib",
  "types": "dist/lib",
  "bin": {
    "niconizer-slack": "dist/bin/index.js"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "fix": "run-p fix:*",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "postinstall": "husky install",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint --ext js,ts .",
    "lint:prettier": "prettier -l \"**/*.{json,md,yml}\"",
    "lint:type": "tsc --noEmit",
    "test": "run-s lint build"
  },
  "dependencies": {
    "@slack/rtm-api": "6.0.0",
    "@slack/web-api": "6.7.2",
    "commander": "9.4.1",
    "emoji-datasource": "14.0.0",
    "slack-message-parser": "2.0.2",
    "ws": "8.9.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.13.0",
    "@typescript-eslint/parser": "5.13.0",
    "eslint": "8.31.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.1",
    "prettier-plugin-packagejson": "2.3.0",
    "rimraf": "3.0.2",
    "typescript": "4.9.4"
  },
  "engines": {
    "node": ">= 14",
    "npm": ">= 7"
  }
}
