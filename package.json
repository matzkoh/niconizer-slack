{
  "name": "niconizer-slack",
  "version": "0.0.0-development",
  "description": "CLI that watch slack messages and send to niconizer.",
  "keywords": [
    "comment",
    "niconizer",
    "slack"
  ],
  "repository": "github:matzkoh/niconizer-slack",
  "license": "MIT",
  "main": "dist/lib",
  "types": "dist/lib",
  "bin": {
    "niconizer-slack": "dist/bin/index.js"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "fix": "run-p fix:*",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "postinstall": "husky install",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint --ext js,ts .",
    "lint:prettier": "prettier -l \"**/*.{json,md,yml}\"",
    "lint:type": "tsc --noEmit",
    "test": "run-s lint build"
  },
  "dependencies": {
    "@slack/rtm-api": "6.0.0",
    "@slack/web-api": "6.2.4",
    "commander": "8.0.0",
    "emoji-datasource": "7.0.2",
    "slack-message-parser": "1.1.0",
    "ws": "7.5.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "4.28.1",
    "@typescript-eslint/parser": "4.28.1",
    "eslint": "7.30.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "husky": "7.0.0",
    "lint-staged": "11.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.3.2",
    "prettier-plugin-packagejson": "2.2.11",
    "rimraf": "3.0.2",
    "typescript": "4.3.5"
  },
  "engines": {
    "node": ">= 14",
    "npm": ">= 7"
  }
}
